{
  "id": "bureaumobileweb",
  "cmd": "java -Xmx1024m -Xdebug -Xrunjdwp:server=y,transport=dt_socket,address=8000,suspend=n -jar /opt/bureau/services/bureaumobileweb.jar  --spring.application.name=XXX_APPNAME_XXX  --spring.cloud.config.profile=XXX_PROFILE_XXX",
  "mem": 2048,
  "cpus": 1.0,
  "instances": XXX_INSTANCES_XXX,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "fsgregistry.azurecr.io/bmwms:XXX_VERSION_XXX",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": XXX_SERVICEPORT_XXX,
          "protocol": "tcp"
        },
        {
          "containerPort": 8000,
          "hostPort": 0,
          "servicePort": XXX_DEBUGPORT_XXX,
          "protocol": "tcp"
        }
      ]
    }
  },
  "uris": [
    "file:///mnt/fsghaenvironments/docker.tar.gz"
  ],
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/bureaumobileweb/rest/rateboard/refined/NZ/",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 2,
      "timeoutSeconds": 20
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external"
  }
}