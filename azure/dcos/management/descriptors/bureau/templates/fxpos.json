{
  "id": "fx-pos-web",
  "mem": 1024,
  "cpus": 1.0,
  "instances": XXX_INSTANCES_XXX,
  "container": {
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/etc/localtime",
        "hostPath": "/etc/localtime",
        "mode": "RW"
      },
      {
        "containerPath": "/usr/local/tomcat/alterconf",
        "hostPath": "/mnt/fsghaenvironments/fx/pos/XXX_STAGE_XXX",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "fsgregistry.azurecr.io/spfxpos:XXX_VERSION_XXX",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8090,
          "servicePort": XXX_SERVICEPORT_XXX,
          "protocol": "tcp"
        },
        {
          "containerPort": 8000,
          "hostPort": 0,
          "servicePort": XXX_DEBUGPORT_XXX,
          "protocol": "tcp"
        }
      ]
    }
  },
  "uris": [
    "file:///mnt/fsghaenvironments/docker.tar.gz"
  ],
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/fx-pos-web/",
      "gracePeriodSeconds": 180,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 2,
      "timeoutSeconds": 30
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_STICKY": "true"
  }
}