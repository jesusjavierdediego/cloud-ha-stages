{
  "id": "fx-pos-web",
  "cmd": "CATALINA_OPTS='-Xdebug -Xrunjdwp:transport=dt_socket,address=8000,server=y,suspend=n'",
  "mem": 1024,
  "cpus": 2.0,
  "instances": XXX_INSTANCES_XXX,
  "container": {
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/etc/localtime",
        "hostPath": "/etc/localtime",
        "mode": "R"
      }
    ],
    "docker": {
      "image": "fsgregistry.azurecr.io/fx-pos-web:XXX_VERSION_XXX",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": XXX_SERVICEPORT_XXX,
          "protocol": "tcp"
        },
        {
          "containerPort": 8000,
          "hostPort": 0,
          "servicePort": XXX_DEBUGPORT_XXX,
          "protocol": "tcp"
        }
      ]
    }
  },
  "uris": [
    "file:///mnt/fsghaenvironments/docker.tar.gz"
  ],
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/fx-ms-web/",
      "gracePeriodSeconds": 5,
      "intervalSeconds": 5,
      "maxConsecutiveFailures": 2,
      "timeoutSeconds": 20
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_STICKY": "true"
  }
}