{
  "id": "bureaumobileweb",
  "cmd": "java -Xmx1024m -Xdebug -Xrunjdwp:server=y,transport=dt_socket,address=8000,suspend=n -jar /opt/bureau/services/bureaumobileweb.jar  --spring.application.name=bmw  --spring.cloud.config.profile=qat",
  "mem": 1024,
  "cpus": 2.0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "fsgregistry.azurecr.io/bmwms:0.1.99-SNAPSHOT",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": 10003,
          "protocol": "tcp"
        },
        {
          "containerPort": 8000,
          "hostPort": 0,
          "servicePort": 8000,
          "protocol": "tcp"
        }
      ]
    }
  },
  "uris": [
    "file:///mnt/fsghaenvironments/docker.tar.gz"
  ],
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/bureaumobileweb/",
      "gracePeriodSeconds": 5,
      "intervalSeconds": 5,
      "maxConsecutiveFailures": 2,
      "timeoutSeconds": 20
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external"
  }
}