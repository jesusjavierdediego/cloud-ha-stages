{
  "id": "notificationserver",
  "cmd": "java -jar /opt/bureau/services/notificationserver.jar  --spring.application.name=notificationserver  --spring.cloud.config.profile=cit",
  "mem": 1024,
  "cpus": 1.0,
  "instances": XXX_INSTANCES_XXX,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "fsgregistry.azurecr.io/notificationserver:XXX_VERSION_XXX",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": XXX_SERVICEPORT_XXX,
          "protocol": "tcp"
        },
        {
          "containerPort": 8000,
          "hostPort": 0,
          "servicePort": XXX_DEBUGPORT_XXX,
          "protocol": "tcp"
        }
      ]
    }
  },
  "uris": [
    "file:///mnt/fsghaenvironments/docker.tar.gz"
  ],
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/api/v1/notification/healthcheck",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 2,
      "timeoutSeconds": 30
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external"
  }
}