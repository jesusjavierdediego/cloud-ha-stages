{
  "id": "bureaumobileweb",
  "cmd": "java -Xdebug -Xrunjdwp:server=y,transport=dt_socket,address=8000,suspend=n -jar /opt/bureau/services/bureaumobileweb.jar  --spring.application.name=bmw  --spring.cloud.config.profile=dev",
  "mem": 512,
  "cpus": 1.0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "fsgregistry.azurecr.io/bureaumobileweb:0.1.110",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0,
          "servicePort": 8080,
          "protocol": "tcp"
        },
        {
          "containerPort": 8000,
          "hostPort": 0,
          "servicePort": 8000,
          "protocol": "tcp"
        }
      ]
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/bureaumobileweb/"
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external"
  }
}